{
  "expo": {
    "name": "drinkly-user-eas",
    "slug": "DrinklyApp",
    "version": "1.1.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.annieyang1.drinklyusereas",
      "infoPlist": {
        "NSCameraUsageDescription": "Enable Camera Acess so that you can record  Videos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.annieyang1.drinklyusereas"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": [
            "merchant.com.drinklyinc"
          ],
          "enableGooglePay": false
        }
      ]
    ]
  }
}
